<div class="container border col-lg-4 col-sm-6 mt-4">
  <%= simple_form_for([@bathroom, @booking]) do |f|%>
    <div>
      <%= f.input :start_date_time, html5: true %>
    </div>
    <%= f.input :end_date_time, html5: true %>
    <div class="button col-lg-5 mb-3"><%= f.button :submit %></div>
  <% end %>
</div>
